{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/me/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/users/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/admin/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/how-to-use/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
